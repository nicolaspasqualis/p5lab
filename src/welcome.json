                    "id": "editor-0",
                    "code": "let insideRadius = 100;\nlet outsideRadius = 150;\n\nconst {points, button, someText} = p5lab.controls({\n  points: {\n    type: \"range\", min: 2, max: 12, step: 1, \n    value: 3,\n  },\n  morePointz: {\n    type: \"range\", min: 2, max: 12, step: 1, \n    value: 3,\n  },\n  color: {\n    type: \"color\", min: 2, max: 12, step: 1, \n    value: \"#ff00ff\",\n  },\n  checkbox: {\n    type: \"checkbox\", initialValue: false,\n  },\n  color2: {\n    type: \"select\", options: [\"color\", \"white\", \"black\"], \n    value: \"color\",\n  },\n  button: {\n    type: 'button', onChange: () => { console.log(\"BUTTTTONNNNEE\") },\n  },\n  someText: { type: 'text', value: \"hi!!\"}\n})\n\nfunction setup() {\n  createCanvas(windowWidth, windowHeight);\n  angleMode(DEGREES);\n  colorMode(HSB);\n}\n\nfunction windowResized() {\n  resizeCanvas(windowWidth, windowHeight);\n}\n\nfunction draw() {\n  background(255);\n  noStroke()\n  fill(0)\n  text(someText.value, 20, 20)\n  stroke(255);\n\n  const centerX = width / 2;\n  const centerY = height / 2;\n  const pointCount = round(points.value);\n  const angleStep = (frameCount*0.1);\n\n  let angle = 0;\n\n  beginShape(TRIANGLE_STRIP);\n  for (let i = 0; i <= pointCount; i += 1) {\n    fill(angle%255, 255, 255);\n    vertex(\n      centerX + cos(angle) * outsideRadius,\n      centerY + sin(angle) * outsideRadius,\n    );\n    angle += angleStep;\n\n    fill(angle%255, 255, 255);\n    vertex(\n      centerX + cos(angle) * insideRadius,\n      centerY + sin(angle) * insideRadius,\n    );\n    angle += angleStep;\n  }\n  endShape();\n}"
                },
